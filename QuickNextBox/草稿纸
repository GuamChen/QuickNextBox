    UIButton *btn3 = [UIButton buttonWithType:UIButtonTypeSystem];
    btn3.titleLabel.font = [UIFont systemFontOfSize:40];
    [btn3 setTitleColor:[UIColor blackColor] forState:UIControlStateNormal];
    btn3.backgroundColor = [UIColor colorWithWhite:0.95 alpha:1];
    [btn3 setImage:[UIImage imageNamed:@"jixiaomubiao"] forState:UIControlStateNormal];
    [btn3 setTitle:@"baimao" forState:UIControlStateNormal];

    [self.view addSubview: btn3];
    [btn3 mas_makeConstraints:^(MASConstraintMaker *make) {
        make.size.mas_equalTo(CGSizeMake(200, 200));
                make.centerX.mas_equalTo(0);
                make.centerY.mas_equalTo(40);
    }];
